{
  "name": "webhook-store",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenWebhook/webhook-store.git"
  },
  "homepage": "https://github.com/OpenWebhook/webhook-store",
  "description": "",
  "author": "",
  "private": true,
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings 0",
    "test": "dotenv -e .env.test -- jest --silent",
    "test:watch": "dotenv -e .env.test -- jest --watch",
    "test:cov": "dotenv -e .env.test -- jest --silent --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest",
    "pretest:ci": "dotenv -e .env.test -- npx prisma migrate dev",
    "test:ci": "dotenv -e .env.test -- jest --silent --coverage"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@nestjs/apollo": "^11.0.3",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.3.10",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.10",
    "@nestjs/event-emitter": "^1.4.0",
    "@nestjs/graphql": "^11.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.10",
    "@nestjs/platform-ws": "^9.3.10",
    "@nestjs/websockets": "^9.3.10",
    "@prisma/client": "^4.11.0",
    "apollo-server-express": "^3.12.0",
    "aws-sdk": "^2.1183.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "express-xml-bodyparser": "^0.3.0",
    "fp-ts": "^2.12.1",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "http-proxy-middleware": "^2.0.6",
    "io-ts": "^2.2.18",
    "jwks-rsa": "^2.1.4",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.10",
    "@types/express": "^4.17.13",
    "@types/express-xml-bodyparser": "^0.3.2",
    "@types/jest": "27.4.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-http": "^0.3.9",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "prisma": "^4.11.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
