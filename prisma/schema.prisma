generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearch"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Webhook {
  id             String   @id
  body           Json
  headers        Json
  ip             String
  path           String
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  host           String
  searchablePath String
}
